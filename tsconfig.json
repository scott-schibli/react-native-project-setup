{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react-native",
        "lib": ["dom", "esnext"],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": true,
        "paths": {
            "components/*": ["./src/components/*"],
            "screens/*": ["./src/screens/*"],
            "styles/*": ["./src/styles/*"],
            "redux/*": ["./src/redux/*"],
            "assets/*": ["./src/assets/*"]
        }
    }
}
